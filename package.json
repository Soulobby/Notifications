{
	"name": "@soulobby/notifications",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "tsc",
		"test": "tsc --noEmit",
		"format": "prettier --write . && eslint --fix .",
		"lint": "prettier --check . && eslint .",
		"dev": "tsx --env-file=.env source/index.ts",
		"start": "node --enable-source-maps --env-file=.env distribution/index.js",
		"update": "pnpm update --interactive --latest"
	},
	"dependencies": {
		"runescape": "^0.5.1",
		"undici": "^6.0.1"
	},
	"devDependencies": {
		"eslint": "^8.54.0",
		"eslint-config-neon": "^0.1.57",
		"prettier": "^3.1.0",
		"tsx": "^4.6.2",
		"typescript": "^5.3.2"
	},
	"packageManager": "pnpm@8.11.0",
	"engines": {
		"node": ">=20.6.0"
	}
}
